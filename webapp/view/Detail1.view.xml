<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="com.masterdetail.controller.Detail1"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="{orderName}" navButtonPress="onNavBack" showNavButton="{device>/system/phone}">
		<content>
			<List id="products" items="{products}" headerText="Products">
				<items>
					<StandardListItem title="{productName}" type="Active" icon = "{productURL}" press="onSelectionChange"/>
				</items>
			</List>
		</content>
	</Page>
</mvc:View>